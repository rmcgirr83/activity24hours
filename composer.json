{
    "name": "rmcgirr83/activity24hours",
    "type": "phpbb-extension",
    "description": "Extension provides a list of all registered users who have visited the board in the last 24 hours as well as displays stats on the number of new posts, new topics and new users within the last 24 hours",
    "version": "1.3.2",
    "time": "2023-10-11",
    "keywords": [
        "phpbb",
        "extension",
        "topics",
        "posts",
        "stats",
        "24 hours"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "24 Hour Activity Stats",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/24_hour_activity_stats",
            "filename": "version_check",
            "ssl": true
        }
    }
}